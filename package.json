{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:format && npm run test:types && npm run test:lint && npm run test:deps && npm run test:unit",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:deps": "depcruise --config ./dependency-cruiser.config.cjs ./src",
    "test:format": "npm run format:check",
    "test:lint": "npm run lint",
    "deps:graph": "depcruise --config ./dependency-cruiser.config.cjs ./src --include-only '^src' --exclude '(.factory|test(s?)|spec(s?))' --collapse 5  --output-type dot | dot -T svg > ./dependency-graph.svg",
    "vitest:watch": "vitest watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --format=codeframe --max-warnings=0 --ext=js,jsx,ts,tsx ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "factory-t": "^0.3.1",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "test-http-client": "^1.0.2",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "devDependencies": {
    "dependency-cruiser": "^16.4.2",
    "eslint-formatter-codeframe": "^7.32.1",
    "msw": "^2.6.6"
  }
}
